# Telegram Bot Token (BotFather'dan oling)
TELEGRAM_BOT_TOKEN=your_bot_token_here

# Google Gemini API Key (https://makersuite.google.com/app/apikey)
GEMINI_API_KEY=your_gemini_api_key_here

# Gemini Model nomi
GEMINI_MODEL=gemini-1.5-flash

# Admin foydalanuvchilar ID'lari (vergul bilan ajratilgan)
# Telegram'da @userinfobot orqali ID'ni bilib olish mumkin
ADMIN_IDS=123456789,987654321

# Admin kanal ID'si (BUSINESS tarif uchun)
# Kanalni yaratib, botni admin qiling va ID'ni oling
ADMIN_CHANNEL_ID=-1001234567890
